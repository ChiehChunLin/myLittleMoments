<div class="bg-light p-3 baby-list">
  <ul class="list-group" id="friendList">
    <% for(let i=0; i< follows.length; i++){ %>
    <li class="list-group-item friend-item" data-friend="<%=follows[i].id%>">
      <img
        src="<%=follows[i].headshot%>"
        alt="babyshot"
        style="
          width: 40px;
          height: 40px;
          border-radius: 50%;
          margin-right: 1rem;
        "
      />
      <%=follows[i].name%>
    </li>
    <% }%>
    <li class="list-group-item text-center">
      <button class="btn btn-primary" id="addFriendBtn">Add Baby</button>
    </li>
  </ul>
</div>
